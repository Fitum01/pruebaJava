            </TableRow>

            <TableRow
                android:paddingTop="10dp"
                android:paddingBottom="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txtT8COV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
